{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "dist/esm",
    "incremental": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "removeComments": false,
    "declaration": true,
    "moduleResolution": "node",
    "preserveConstEnums": true
  },
  "include": [
    "lib"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
